{
  "manifest_version": 3,
  "name": "willPreisProM2haben",
  "version": "1.1.0",
  "description": "Shows price per square meter directly on willhaben.at listings.",
  "icons": {
    "16": "icon-128x128.png",
    "48": "icon-128x128.png",
    "128": "icon-128x128.png"
  },
  "permissions": [],
  "host_permissions": [
    "*://www.willhaben.at/iad/immobilien/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://www.willhaben.at/iad/immobilien/*"
      ],
      "js": [
        "contentScript.js"
      ],
      "css": [
        "styles.css"
      ]
    }
  ]
}